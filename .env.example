# AWS Configuration
# Replace these placeholder values with your actual AWS credentials
# DO NOT commit your actual credentials to version control!
AWS_ACCESS_KEY_ID=AKIAXXXXXXXXXXXXXXXX        # 20-character AWS Access Key ID, starts with 'AKIA'
AWS_SECRET_ACCESS_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX    # 40-character AWS Secret Access Key
AWS_DEFAULT_REGION=us-east-1                  # AWS Region (e.g., us-east-1, us-west-2)
AWS_ACCOUNT_ID=123456789012                   # 12-digit AWS Account ID

# Application Configuration
APP_ENV=development                           # Environment (development, staging, production)
DEBUG=true                                    # Enable debug mode (true/false)

# API Configuration
API_PORT=3000                                 # Port number for the API server
API_HOST=localhost                            # API host address

# Other Settings
LOG_LEVEL=debug                               # Logging level (debug, info, warn, error)
